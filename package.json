{
	"name": "overpass2geojson",
	"version": "0.0.0",
	"license": "MIT",
	"bin": "dist/cli.cjs",
	"engines": {
		"node": ">=16"
	},
	"scripts": {
		"build": "rspack build",
		"build:sea:windows": "scripts\\build-sea.bat",
		"build:sea:unix": "./scripts/build-sea.sh",
		"build:sea:macos": "./scripts/build-sea.sh",
		"build:sea:linux": "./scripts/build-sea.sh",
		"dev": "rspack build --watch",
		"test": "prettier --check . && xo && ava"
	},
	"files": [
		"dist"
	],
	"dependencies": {
		"ink": "^4.1.0",
		"react": "^18.2.0"
	},
	"devDependencies": {
		"@rspack/cli": "^1.5.8",
		"@rspack/core": "^1.5.8",
		"@sindresorhus/tsconfig": "^3.0.1",
		"@types/react": "^18.0.32",
		"@vdemedes/prettier-config": "^2.0.1",
		"ava": "^5.2.0",
		"chalk": "^5.2.0",
		"eslint-config-xo-react": "^0.27.0",
		"eslint-plugin-react": "^7.32.2",
		"eslint-plugin-react-hooks": "^4.6.0",
		"fullscreen-ink": "^0.1.0",
		"ink-testing-library": "^3.0.0",
		"postject": "^1.0.0-alpha.6",
		"prettier": "^2.8.7",
		"ts-node": "^10.9.1",
		"typescript": "^5.0.3"
	},
	"ava": {
		"extensions": {
			"ts": "module",
			"tsx": "module"
		},
		"nodeArguments": [
			"--loader=ts-node/esm"
		]
	},
	"xo": {
		"extends": "xo-react",
		"prettier": true,
		"rules": {
			"react/prop-types": "off"
		}
	},
	"prettier": "@vdemedes/prettier-config",
	"packageManager": "yarn@4.10.3"
}
